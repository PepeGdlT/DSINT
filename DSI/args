query "getPoseer" (Heroe heroe, Objeto objeto)
    $p : Poseer(sujeto == heroe, poseido == objeto)
  
end

query "getLiberar" (Personaje libertador, Personaje liberado )
    $l : Liberar(sujeto == libertador, liberado == liberado)
end